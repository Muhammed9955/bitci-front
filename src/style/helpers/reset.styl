body, body *
  font-family Open Sans!important
  margin 0
  padding 0
  border 0
  list-style: none
  outline: 0

body
  .h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6
    margin 0
  &.has-modal
    overflow: hidden

body
  background #F9F9F9 !important

.justify-center
  justify-content: center

a
  text-decoration none!important
  cursor pointer

.full-width
  width 100%!important
